\par \pard\plain \ltrpar\s1\cf0{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\cbpat2\ql\rtlch\af7\afs24\lang255\ltrch\dbch\af3\langfe255\hich\f7\fs24\lang1051\loch\f7\fs24\lang1051 {\rtlch \ltrch\loch\f7\fs24\lang1051\i0\b0 Pred posledn\u253\'3fm slovom odtiahol Emo r\u225\'3ff od br\u250\'3fsa a obna\u382\'3fil tak v\u253\'3fkrik \u8222\'3fpeniaze\'81\'67.}
\par \pard\plain \ltrpar\s1\cf0{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\cbpat2\ql\rtlch\af7\afs24\lang255\ltrch\dbch\af3\langfe255\hich\f7\fs24\lang1051\loch\f7\fs24\lang1051 {\rtlch \ltrch\loch\f7\fs24\lang1051\i0\b0 Otec sa obr\u225\'3ftil. \'81\'5c Na \u269\'3fo? Na u\u269\'3fenie?}
