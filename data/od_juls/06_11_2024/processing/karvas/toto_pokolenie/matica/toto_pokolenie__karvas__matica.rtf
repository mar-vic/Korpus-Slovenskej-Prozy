\par \pard\plain \ltrpar\s1\cf0{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\cbpat2\ql\rtlch\af7\afs24\lang255\ltrch\dbch\langfe255\hich\f7\fs24\lang1033\loch\f7\fs24\lang1033{\rtlch \ltrch\loch\f7\fs24\lang1033\i0\b0{\cf1\rtlch\ltrch\hich\f4\loch\f4 <h1>}}{\rtlch \ltrch\loch\f7\fs24\lang1033\i0\b0{\cf1\rtlch\ltrch\hich\b\loch\b II.}}
